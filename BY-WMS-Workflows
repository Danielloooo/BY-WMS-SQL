[SELECT DISTINCT SERVICE_INSTRUCTIONS.SERV_ID SERVICE_ID,
        service_instructions.serv_ins_id SERVICE_INSTRUCTION_ID,
        service_instructions.seqnum,
        service_instructions.serv_ins_typ SERVICE_INSTRUCTION_TYPE,
        service_instructions.stop_serv_flg,
        WAREHOUSE_SERVICES.SEQNUM WORKFLOW_FIRE_SEQUENCE,
        WAREHOUSE_SERVICES.SERV_ENAFLG WORKFLOW_ENABLED,
        --SERVICE_INSTRUCTIONS_ACTION.SERV_INS_ID/*DUPLICATE*/,
        SERVICE_INSTRUCTIONS_ACTION.SERV_ACTION_TYP,
        --WAREHOUSE_SERVICE_EXITPOINT.EXITPNT EXIT_POINT /*added description so comented this line*/,

        (SELECT LNGDSC
           FROM dscmst
          WHERE COLNAM = 'exitpnt'
            and colval = WAREHOUSE_SERVICE_EXITPOINT.EXITPNT) EXIT_POINT_description,
        --SERVICE_INSTRUCTIONS_ACTION.SERV_ACTION_CMD /*NON SELECTABLE COLUMN?*/,
        --WAREHOUSE_SERVICES_EXIT_POINT_MOVE_ZONE.SRC_MOV_ZONE_ID /*added description so comented this line*/,

        CASE WHEN WAREHOUSE_SERVICES_EXIT_POINT_MOVE_ZONE.SRC_MOV_ZONE_ID = '-10000' THEN 'ANY'
             ELSE (SELECT MOV_ZONE_COD
                     FROM mov_zone
                    WHERE MOV_ZONE.mov_zone_id = WAREHOUSE_SERVICES_EXIT_POINT_MOVE_ZONE.SRC_MOV_ZONE_ID)
        END SOURCE_MOVE_ZONE,
        --WAREHOUSE_SERVICES_EXIT_POINT_MOVE_ZONE.DST_MOV_ZONE_ID /*added description so comented this line*/,

        CASE WHEN WAREHOUSE_SERVICES_EXIT_POINT_MOVE_ZONE.DST_MOV_ZONE_ID = '-10000' THEN 'ANY'
             ELSE (SELECT MOV_ZONE_COD
                     FROM mov_zone
                    WHERE MOV_ZONE.mov_zone_id = WAREHOUSE_SERVICES_EXIT_POINT_MOVE_ZONE.DST_MOV_ZONE_ID)
        END DESTINATION_MOVE_ZONE,
        SERVICE_INSTRUCTIONS_ACTION.SEQNUM,
        SERVICE_INSTRUCTIONS_ACTION.RF_FRM_ID,
        SERVICE_INSTRUCTIONS_ACTION.GUI_FRM_ID,
        --SERVICE_INSTRUCTIONS_ACTION.CNFM_VAL_VAR_NAM, --SERVICE_INSTRUCTIONS_ACTION.POST_CNFM_VAL_CMD /*NON SELECTABLE COLUMN?*/,
        SERVICE_INSTRUCTIONS_ACTION.RPT_ID,
        SERVICE_INSTRUCTIONS_ACTION.CMD,
        SERVICE_INSTRUCTIONS_ACTION.HLDCOD,
        SERVICE_INSTRUCTIONS_ACTION.HLDPFX,
        SERVICE_INSTRUCTIONS_ACTION.REACOD
   FROM serv_ins SERVICE_INSTRUCTIONS
   LEFT
   JOIN wh_serv WAREHOUSE_SERVICES
     ON service_instructions.serv_id = warehouse_services.serv_id
   LEFT
   JOIN serv_ins_action SERVICE_INSTRUCTIONS_ACTION
     ON service_instructions.serv_ins_id = service_instructions_action.serv_ins_id
   LEFT
   JOIN
        --exitpnt EXIT_POINT,
        wh_serv_exitpnt WAREHOUSE_SERVICE_EXITPOINT
     ON SERVICE_INSTRUCTIONS.SERV_ID = WAREHOUSE_SERVICE_EXITPOINT.SERV_ID
   LEFT OUTER
   JOIN wh_serv_exitpnt_mov_zone WAREHOUSE_SERVICES_EXIT_POINT_MOVE_ZONE
     ON WAREHOUSE_SERVICE_EXITPOINT.SERV_ID = WAREHOUSE_SERVICES_EXIT_POINT_MOVE_ZONE.SERV_ID
        --AND WAREHOUSE_SERVICE_EXITPOINT.EXITPNT = WAREHOUSE_SERVICES_EXIT_POINT_MOVE_ZONE.EXITPNT
        --cnfrm_bck_serV CONFIRM_BACK_SERVICE
];
